{
  "name": "infocyph/uid",
  "description": "UUID (RFC 4122 + Unofficial/Draft), ULID, Snowflake ID, Sonyflake ID, TBSL (library exclusive) generator!",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "uuid",
    "ulid",
    "snowflake",
    "sonyflake",
    "tbsl",
    "id",
    "key",
    "unique"
  ],
  "authors": [
    {
      "name": "abmmhasan",
      "email": "abmmhasan@gmail.com"
    }
  ],
  "autoload": {
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "Infocyph\\UID\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "ext-bcmath": "*"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "archive": {
    "exclude": [
      "/.github"
    ]
  },
  "replace": {
    "abmmhasan/uuid": "*"
  }
}